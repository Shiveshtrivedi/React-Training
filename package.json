{
  "dependencies": {
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "typescript-eslint": "^8.2.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && node ./src/index.js",
    "lint": "eslint ./task_manager_using_typescript/src/*.{js,ts,tsx}",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "task_manager_using_typescript/src/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
